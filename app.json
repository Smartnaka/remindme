{
  "expo": {
    "name": "RemindMe",
    "slug": "lecture-reminder-app-1elte0d",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "remindme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash-icon-dark.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.remindme.remindme",
      "icon": "./assets/images/ios-light.png",
      "infoPlist": {
        "NSCalendarsUsageDescription": "RemindMe needs access to your calendar to sync your lecture schedule.",
        "NSRemindersUsageDescription": "RemindMe needs access to reminders to set alerts for your lectures."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "app.remindme.remindme",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://remindme.com/"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://remindme.com/"
      },
      "eas": {
        "projectId": "01b5e388-b3d0-4b99-adf0-d12e41ac2c1a"
      }
    }
  }
}